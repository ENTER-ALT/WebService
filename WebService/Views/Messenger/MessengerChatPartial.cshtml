@model SendMessageViewModel
<div>
    <header class="major">
        <h2>Чат пользователей</h2>
    </header>
    <div class="box">
        @for (int i = 0; i < @Model.Chat.Count; i++)
        {
            <p>
                Пользователь @Model.Chat[i].AuthorName.ToString()
                [@Model.Chat[i].DateAdded.ToString()]:
                @Model.Chat[i].MessageText.ToString()
            </p>
        }
    </div>
</div>